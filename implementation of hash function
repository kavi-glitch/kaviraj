#include <stdio.h>
#define SIZE 10

int main() {
    int hash[SIZE], i, key, index;

    // Initialize hash table
    for (i = 0; i < SIZE; i++)
        hash[i] = -1;

    // Insert elements
    for (i = 0; i < 5; i++) {
        printf("Enter key: ");
        scanf("%d", &key);

        index = key % SIZE;

        // Linear probing
        while (hash[index] != -1)
            index = (index + 1) % SIZE;

        hash[index] = key;
    }

    // Display hash table
    printf("\nHash Table:\n");
    for (i = 0; i < SIZE; i++)
        printf("%d -> %d\n", i, hash[i]);

    return 0;
}
