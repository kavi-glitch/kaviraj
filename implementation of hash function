#include <stdio.h>
#include <stdlib.h>
int *h,n,m,c=0;
void ins(int k){
    int i=k%m;
    while(h[i]!=-1) i=(i+1)%m;
    h[i]=k; c++;
}
int main(){
    int i,k;
    printf("\nEnter the number of employee records (N): ");
    scanf("%d",&n);
    printf("\nEnter the two digit memory locations (m) for hash table: ");
    scanf("%d",&m);
    h=(int*)malloc(m*sizeof(int));
    for(i=0;i<m;i++) h[i]=-1;
    printf("\nEnter the four digit key values (K) of N Employee Records:\n");
    for(i=0;i<n;i++){
        scanf("%d",&k);
        if(c<m) ins(k);
    }
  printf("\nHash Table contents are:\n");
    for(i=0;i<m;i++)
        printf("\nT[%d] --> %d",i,h[i]);
    return 0;
